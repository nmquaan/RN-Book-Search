{
  "expo": {
    "name": "RN Book Search",
    "description": "A Book Search book created using React Native",
    "slug": "RNBookSearch",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vnapps.rnBookSearch"
    },
    "android": {
      "package": "com.vnapps.rnBookSearch",
      "permissions": []
    },
    "androidStatusBarColor": "#FAFAFA",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FAFAFA"
    },
    "githubUrl": "https://github.com/vikrantnegi/RN-Book-Search"
  }
}
